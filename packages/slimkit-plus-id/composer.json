{
    "name": "slimkit/plus-id",
    "description": "A \"slimkit/plus-id\" package For ThinkSNS+✈️.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.2.0",
        "illuminate/console": "5.8.*",
        "illuminate/contracts": "5.8.*",
        "illuminate/database": "5.8.*",
        "illuminate/support": "5.8.*"
    },
    "autoload": {
        "psr-4": {
            "SlimKit\\PlusID\\": "src/",
            "SlimKit\\PlusID\\Seeds\\": "database/seeds/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "SlimKit\\PlusID\\Providers\\AppServiceProvider",
                "SlimKit\\PlusID\\Providers\\ModelServiceProvider",
                "SlimKit\\PlusID\\Providers\\RouteServiceProvider"
            ]
        }
    },
    "version": "2.3.2"
}
